		<column name="updateuuid" type="String" />
		<!-- mandatory fields -->
		<column name="contactID" type="String" />
		<column name="contactEmail" type="String" />
		<column name="contactCountry" type="String" />
		<!-- optional fields -->
		<column name="contactFirstName" type="String" />
