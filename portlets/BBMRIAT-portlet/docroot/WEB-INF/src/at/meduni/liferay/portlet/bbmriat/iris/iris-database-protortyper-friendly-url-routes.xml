<routes>
   <route>
        <pattern></pattern>
        <generated-parameter name="mvcPath">/html/iris/irisdatabaseprotortyper/patientlist.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">exclusive</implicit-parameter>
    </route>
    <route>
        <pattern>/patient/{patientid:\d+}</pattern>
        <generated-parameter name="mvcPath">/html/iris/irisdatabaseprotortyper/patient.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">pop_up</implicit-parameter>
    </route>
    <route>
        <pattern>/patient/{patientid:\d+}/examination/{examinationid:\d+}</pattern>
        <generated-parameter name="mvcPath">/html/iris/irisdatabaseprotortyper/examination.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">exclusive</implicit-parameter>
    </route> 
</routes>