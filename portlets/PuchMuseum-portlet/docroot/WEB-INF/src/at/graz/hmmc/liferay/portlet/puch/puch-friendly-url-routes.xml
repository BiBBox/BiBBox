<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
	<route>
        <pattern>/transactionert</pattern>
        <generated-parameter name="mvcPath">/html/transactions/edit.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">pop_up</implicit-parameter>
    </route>
    <route>
        <pattern>/transaction/{puchmuseumsobjectId:\d+}/{transactionId:\d+}/{transactiontype}</pattern>
        <generated-parameter name="mvcPath">/html/puchmuseumsobjekterstellen/edit_transaction_popup.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">pop_up</implicit-parameter>
    </route>
    <route>
        <pattern>/images/{puchmuseumsobjectId:\d+}/{imageId:\d+}</pattern>
        <generated-parameter name="mvcPath">/html/imageupload/edit_image_popup.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">pop_up</implicit-parameter>
    </route>
</routes>