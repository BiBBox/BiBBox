<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
    <route>
        <pattern>/createevent2/{patientId:\d+}</pattern>
        <generated-parameter name="mvcPath">/html/demo2/createevent/createevent.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">pop_up</implicit-parameter>
    </route>
    <route>
        <pattern>/createevent2/{patientId:\d+}/{eventtype:\w+}</pattern>
        <generated-parameter name="mvcPath">/html/demo2/createevent/createevent.jsp</generated-parameter>
        <implicit-parameter name="p_p_state">pop_up</implicit-parameter>
    </route>
</routes>